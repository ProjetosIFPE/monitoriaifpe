<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	metadata-complete="false" id="WebApp_ID" version="3.1">
	<welcome-file-list>
		<welcome-file>sistema.do</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>ServletLogin</servlet-name>
		<jsp-file>jsp/login.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>ServletLogin</servlet-name>
		<url-pattern>/login.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ServletInicial</servlet-name>
		<jsp-file>jsp/TelaInicial.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>ServletInicial</servlet-name>
		<url-pattern>/inicial.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ControladorAcesso</servlet-name>
		<servlet-class>br.com.projetoperiodo.controller.acesso.ControladorAcesso</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ControladorAcesso</servlet-name>
		<url-pattern>/sistema.do</url-pattern>
	</servlet-mapping>
</web-app>
