<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
   
   <hibernate-mapping>
   	<class name="br.com.projetoperiodo.model.instituto.usuario" table="USUARIO">
		   <id name="login" column="USUARIO_LOGIN" type="string">
		   	<generator class="assigned"></generator>
		   </id>
		   <property name="nome" column="USUARIO_NOME" />
		   <property name="senha" column="USUARIO_SENHA" />
		   <property name="email" column="USUARIO_EMAIL" />
	 </class>
   </hibernate-mapping>