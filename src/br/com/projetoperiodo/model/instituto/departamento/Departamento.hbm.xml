<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="br.com.projetoperiodo.model.instituto.departamento.Departamento" table="DEPARTAMENTO">
		<id name="id" column="DEPARTAMENTO_CD">
			<generator class="increment" />
		</id>
		<property name="descricao" column="DEPARTAMENTO_DS" />
		<property name="descricaoAbreviada" column="DEPARTAMENTO_DS_ABREV" />
	</class>
</hibernate-mapping>