<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="br.com.projetoperiodo.model.relatorio.atividade.Atividade" table="ATIVIDADE">
		<id name="id" column="ATIVIDADE_CD">
			<generator class="increment" />
		</id>
		<property name="horario_entrada" column="HORARIO_ENTRADA" />
		<property name="horario_saida" column="HORARIO_SAIDA" />
		<property name="data" column="ATIVIDADE_DATA" />
 	</class>
</hibernate-mapping>